<div class="container">
  <div class="row">
    <% @services.each do |service| %>
    <div class="col-12 col-md-6 col-lg-4">
      <div class="card-trip">
        <%= cl_image_tag service.photo %>
        <div class="card-trip-infos">
          <div>
            <h2><%=link_to service.name, service %></h2>
            <p>evaluation stars!</p>
          </div>
          <h2>$<%= service.price %></h2>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
